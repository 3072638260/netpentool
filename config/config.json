{
  "logging": {
    "level": "INFO",
    "file": "logs/traes.log",
    "max_size": "10MB",
    "backup_count": 5,
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  },
  "network": {
    "default_interface": "auto",
    "timeout": 5,
    "retry_count": 3,
    "packet_delay": 0.1
  },
  "attack": {
    "arp": {
      "interval": 1.0,
      "packet_count": 0,
      "restore_on_exit": true,
      "spoof_mode": "bidirectional",
      "max_targets": 50
    },
    "dhcp": {
      "thread_count": 10,
      "request_rate": 100,
      "mac_randomization": true,
      "vendor_classes": [
        "MSFT 5.0",
        "android-dhcp",
        "dhcpcd"
      ]
    }
  },
  "security": {
    "whitelist": {
      "enabled": true,
      "ips": [
        "192.168.1.1",
        "192.168.1.254"
      ],
      "macs": [],
      "subnets": []
    },
    "rate_limiting": {
      "enabled": true,
      "max_packets_per_second": 100,
      "burst_size": 50
    }
  },
  "output": {
    "save_results": true,
    "results_directory": "data/results",
    "format": "json",
    "include_timestamps": true,
    "compress_logs": false
  },
  "performance": {
    "max_memory_usage": "512MB",
    "cpu_limit_percent": 80,
    "disk_space_threshold": "1GB"
  }
}